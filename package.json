{
  "name": "aegon-technical-design-library",
  "version": "0.0.1",
  "description": "The library will host the HTML CSS & JS resources required for cXstudio and third parties to implement in projects.",
  "main": "README.md",
  "author": "Rocco R. <rocco.russo@hcl.com>",
  "license": "no license",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/cxstudio/aegon-technical-design-library.git"
  },
  "scripts": {
    "deps": "npm install && bower install",
    "subsetup": "git submodule foreach 'echo \">> npm and bower install: Done\" `npm install && bower install`'",
    "subs": "git submodule update --init --recursive && npm run subsetup",
    "setup": "npm run deps && npm run subs",
    "update": "git pull && npm run subs",
    "build": "gulp",
    "serve": "gulp --dev",
    "pull": "git pull && git submodule foreach 'echo \">> force git pull in submodules: Done\" `git checkout master && git pull`'"
  },
  "keywords": [
    "toolkit",
    "framework",
    "scaffold",
    "patterns",
    "library",
    "sass",
    "scss",
    "cXstudio",
    "design-library",
    "hcl",
    "ui-elements",
    "layout",
    "grid",
    "sass",
    "responsive",
    "rwd",
    "semantic"
  ],
  "homepage": "https://www.aegon.nl",
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "browser-sync": "^1.7.1",
    "browserify": "^6.3.2",
    "change-case": "^2.1.5",
    "cheerio": "^0.18.0",
    "del": "^0.1.3",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-concat": "~2.4.3",
    "gulp-header": "~1.2.2",
    "gulp-if": "~1.2.2",
    "gulp-imagemin": "^2.0.0",
    "gulp-jasmine": "^2.3.0",
    "gulp-jshint": "^1.11.2",
    "gulp-notify": "^2.2.0",
    "gulp-order": "~1.1.1",
    "gulp-plumber": "^0.6.5",
    "gulp-rename": "~1.2.0",
    "gulp-sass": "^2.1.1",
    "gulp-sftp": "~0.1.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.6",
    "gulp-watch": "~4.2.0",
    "handlebars": "^2.0.0",
    "js-beautify": "^1.5.4",
    "jshint-stylish": "^2.0.1",
    "junk": "^1.0.0",
    "marked": "^0.3.2",
    "mkdirp": "~0.5.0",
    "mkpath": "~0.1.0",
    "q": "^1.1.2",
    "run-sequence": "^1.0.1",
    "through2": "^0.6.3",
    "vinyl-map": "~1.0.1"
  }
}
